{
  "jsc": {
    "parser": {
      "syntax": "ecmascript", 
      "tsx": true,
      "jsx": true,
      "decorators": false
    },
    "transform": {
      "react": {
        "runtime": "automatic",
        "refresh": true
      }
    },
    "target": "es2020",
    "loose": false
  },
  "module": {
    "type": "es6"
  },
  "minify": false,
  "sourceMaps": true
}
